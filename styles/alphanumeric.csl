  <info>
    <title>Alphanumeric</title>
    <id>http://typst.org/csl/alphanumeric</id>
    <author>
      <name>Martin Haug</name>
      <email>martin.haug@typst.app</email>
    </author>
    <author>
      <name>Laurenz MÃ¤dje</name>
      <email>laurenz.maedje@typst.app</email>
